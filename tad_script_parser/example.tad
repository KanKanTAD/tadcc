domain Customer {
	id :string @key
	name :string 
	telephone :string
}

domain Order {
	customer_id :string @feign(Customer::id)
}

domain Phone {
	to_call():void {}
	get():Phone {}
}

base :Phone = new Phone()

phone :Phone = link<base> Phone()

phone->to_call()

asy (p:=phone->Phone()){
	p->to_call()
} then (){
}



